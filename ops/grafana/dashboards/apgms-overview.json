        "defaults": { "custom": { "drawStyle": "lines" } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (service, stage) (increase(apgms_release_failures_total[1h]))",
          "legendFormat": "{{service}} {{stage}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 28 }
    }
